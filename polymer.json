{
  "basePath": "/powercenter/",
  "entrypoint": "app/index.html",
  "shell": "app/elements/stryd-app.html",
  "sources": [
    "app/elements/**",
    "app/fonts/**",
    "app/images/**",
    "app/scripts/**",
    "app/styles/**"
  ],
  "extraDependencies": [
    "app/manifest.json",
    "app/bower_components/webcomponentsjs/custom-elements-es5-adapter.js",
    "app/bower_components/webcomponentsjs/webcomponents-hi-ce.js",
    "app/bower_components/webcomponentsjs/webcomponents-hi-sd-ce.js",
    "app/bower_components/webcomponentsjs/webcomponents-hi-sd.js",
    "app/bower_components/webcomponentsjs/webcomponents-hi.js",
    "app/bower_components/webcomponentsjs/webcomponents-lite.js",
    "app/bower_components/webcomponentsjs/webcomponents-loader.js",
    "app/bower_components/webcomponentsjs/webcomponents-sd-ce.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "basePath": "/powercenter/",
    "bundle": true,
    "js": {
      "compile": {
        "exclude": [
          "*.min.js",
          "*/external.js",
          "*/d3.js",
          "*/moment.js",
          "*/amcharts/*.js"
        ]
      },
      "minify": {
        "exclude": [
          "*.min.js",
          "*/external.js",
          "*/d3.js",
          "*/moment.js",
          "*/amcharts/*.js"
        ]
      }
    },
    "css": {
      "minify": {
        "exclude": [
          "*.min.css"
        ]
      }
    },
    "html": {"minify": true}
  }]
}